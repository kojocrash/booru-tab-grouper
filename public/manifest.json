{
  "manifest_version": 3,
  "name": "Booru Tab Grouper",
  "version": "2.3.0",
  "description": "Attempts to group Booru tabs by artist/primary tag.",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png",
    "512": "icons/icon_512.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Booru Tab Grouper",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "windows",
    "tabGroups"
  ]
}