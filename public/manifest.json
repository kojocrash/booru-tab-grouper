{
  "manifest_version": 3,
  "name": "Gelbooru Tab Grouper",
  "version": "0.1.0",
  "description": "Group Gelbooru tabs by artist",
  "icons": {
    "16": "icons/icon_16.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Gelbooru Tab-grouper",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}